


<h1>Create Car Workshop</h1>
<hr/>
<div class="row">
    <div class="col-md-4">
        <form action="/CarWorkshop/Create" method="post">
            <div class="form-group mb-3">
                <label class="control-label" for="Name">Name</label>
                <input class="form-control" type="text" id="Name" name="Name"></input>
            </div>
            <div class="form-group mb-3">
                <label class="control-label" for="About">About</label>
                <input class="form-control" type="text" id="About" name="About"></input>
            </div>
            <div class="form-group mb-3">
                <label class="control-label" for="Description">Description</label>
                <input class="form-control" type="text" id="Description" name="Description"></input>
        <div class="form-group mb-3">
                <label class="control-label" for="PhoneNumber">Phone Number</label>
                <input class="form-control" type="text" id="PhoneNumber" name="ContactDetails.PhoneNumber"></input>
            </div>
            <div class="form-group mb-3">
                <label class="control-label" for="Street">Street</label>
                <input class="form-control" type="text" id="Street" name="ContactDetails.Street"></input>
            </div>
            <div class="form-group mb-3">
                <label class="control-label" for="City">City</label>
                <input class="form-control" type="text" id="City" name="ContactDetails.City"></input
            <div class="form-group mb-3">
                <label class="control-label" for="PostalCode">Postal Code</label>
                <input class="form-control" type="text" id="PostalCode" name="ContactDetails.PostalCode"></input>
            </div>
            <div class="form-group mb-3">
                <input type="submit" value="Create" class="btn btn-primary"></input>
            </div>

        </form>
    </div>
</div>